{
    "name": "pkmn-friends",
    "description": "www.pokemon-friends.com",
    "author": "Antoine Benevaut",
    "homepage": "https://www.pokemon-friends.com",
    "bugs": "https://github.com/pokemon-friends-com/www/issues",
    "private": true,
    "main": "resources/js/app.js",
    "engines": {
        "node": "^12"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mocha": "cross-env NODE_ENV=testing mochapack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/bootstrap.js tests/Unit/AppJs/\\\\*.spec.js",
        "mocha-watch": "cross-env NODE_ENV=testing mochapack --watch --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/bootstrap.js tests/Unit/AppJs/\\\\*.spec.js",
        "test": "cross-env NODE_ENV=testing nyc npm run mocha",
        "testim-prod": "node_modules/@testim/testim-cli/cli.js --token \"${TESTIM_TOKEN}\" --project \"${TESTIM_PROJECT}\" --grid \"Testim-Grid\" --report-file testim-report.xml",
        "testim-dev": "node_modules/@testim/testim-cli/cli.js --token \"${TESTIM_TOKEN}\" --project \"${TESTIM_PROJECT}\" --use-local-chrome-driver --base-url 127.0.0.1:8000"
    },
    "devDependencies": {
        "@abenevaut/gameforest": "^6.0.2",
        "@sentry/browser": "^5.16.1",
        "@sentry/integrations": "^5.16.1",
        "@sentry/types": "^5.16.0",
        "@testim/testim-cli": "^3.72.0",
        "@vue/babel-preset-app": "^3.11.0",
        "@vue/test-utils": "^1.0.3",
        "admin-lte": "^3.0.5",
        "axios": "^0.19",
        "babel-plugin-istanbul": "^6.0.0",
        "bootstrap": "^4.5.0",
        "bootstrap-select": "^1.13.17",
        "clipboard": "^2.0.6",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.2",
        "datatables": "^1.10.18",
        "datatables.net-bs4": "^1.10.21",
        "dotenv": "^8.2.0",
        "easy-pie-chart": "^2.1.7",
        "ekko-lightbox": "^5.3.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-lodash": "^7.1.0",
        "eslint-plugin-no-jquery": "^2.4.1",
        "eslint-plugin-vue": "^6.2.2",
        "expect": "^26.0.1",
        "expose-loader": "^0.7.5",
        "false": "^0.0.4",
        "favicons-webpack-plugin": "^3.0.1",
        "flatpickr": "^4.6.3",
        "holderjs": "^2.9.7",
        "imagehover.css": "^2.0.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "jquery": "^3.5.1",
        "jquery-parallax.js": "^1.5.0",
        "jquery-sticky": "^1.0.4",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "laravel-echo": "^1.8.0",
        "laravel-mix": "^5.0.4",
        "lazysizes": "^5.2.2",
        "lodash": "^4.17.13",
        "mocha": "^5.2.0",
        "mochapack": "^1.1.15",
        "mock-echo": "^1.1.1",
        "moment": "^2.26.0",
        "nouislider": "^14.5.0",
        "nyc": "^15.1.0",
        "owl.carousel": "^2.3.4",
        "popper.js": "^1.16",
        "pusher-js": "^6.0.3",
        "qrcode": "^1.4.4",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.7",
        "sass-loader": "8.*",
        "sinon": "^9.0.2",
        "stylelint": "^13.5.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-scss": "^3.17.2",
        "stylelint-webpack-plugin": "^2.0.0",
        "tempusdominus-core": "^5.0.3",
        "vanilla-lazyload": "^15.2.0",
        "vue": "^2.6.11",
        "vue-i18n": "^8.18.1",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {}
}
