{
    "name": "pkmn-friends",
    "version": "1.2.10",
    "description": "www.pokemon-friends.com",
    "author": "Antoine Benevaut",
    "homepage": "https://www.pokemon-friends.com",
    "bugs": "https://github.com/pokemon-friends-com/www/issues",
    "private": true,
    "main": "resources/js/app.js",
    "engines": {
        "node": "^12"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mocha": "cross-env NODE_ENV=testing mochapack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/bootstrap.js tests/Unit/AppJs/\\\\*.spec.js",
        "mocha-watch": "cross-env NODE_ENV=testing mochapack --watch --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/bootstrap.js tests/Unit/AppJs/\\\\*.spec.js",
        "test": "cross-env NODE_ENV=testing nyc npm run mocha",
        "prod:testim": "node_modules/@testim/testim-cli/cli.js --token \"${TESTIM_TOKEN}\" --project \"${TESTIM_PROJECT}\" --grid \"Testim-Grid\" --report-file testim-report.xml",
        "dev:testim": "node_modules/@testim/testim-cli/cli.js --token \"${TESTIM_TOKEN}\" --project \"${TESTIM_PROJECT}\" --use-local-chrome-driver --base-url 127.0.0.1:8000"
    },
    "devDependencies": {
        "@abenevaut/gameforest": "^6.0.2",
        "@sentry/browser": "^5.22.3",
        "@sentry/integrations": "^5.27.3",
        "@sentry/types": "^5.22.3",
        "@testim/testim-cli": "^3.118.0",
        "@vue/babel-preset-app": "^3.11.0",
        "@vue/test-utils": "^1.0.5",
        "axios": "^0.20",
        "babel-plugin-istanbul": "^6.0.0",
        "bootstrap": "^4.5.3",
        "bootstrap-select": "^1.13.18",
        "canvas": "^2.6.1",
        "clipboard": "^2.0.6",
        "copy-webpack-plugin": "^5.1.2",
        "cross-env": "^7.0.2",
        "datatables": "^1.10.18",
        "datatables.net-bs4": "^1.10.22",
        "dotenv": "^8.2.0",
        "easy-pie-chart": "^2.1.7",
        "ekko-lightbox": "^5.3.0",
        "eslint": "^7.10.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-lodash": "^7.1.0",
        "eslint-plugin-no-jquery": "^2.5.0",
        "eslint-plugin-vue": "^7.1.0",
        "expect": "^26.6.1",
        "expose-loader": "^0.7.5",
        "false": "^0.0.4",
        "favicons-webpack-plugin": "^4.2.0",
        "flatpickr": "^4.6.6",
        "holderjs": "^2.9.7",
        "imagehover.css": "^2.0.0",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "jquery": "^3.5.1",
        "jquery-parallax.js": "^1.5.0",
        "jquery-sticky": "^1.0.4",
        "jsdom": "^16.4.0",
        "jsdom-global": "^3.0.2",
        "laravel-echo": "^1.8.1",
        "laravel-mix": "^5.0.7",
        "lazysizes": "^5.2.2",
        "lodash": "^4.17.20",
        "mocha": "^8.1.3",
        "mochapack": "^2.0.3",
        "mock-echo": "^1.1.1",
        "moment": "^2.29.0",
        "nouislider": "^14.6.2",
        "nyc": "^15.1.0",
        "owl.carousel": "^2.3.4",
        "popper.js": "^1.16",
        "pusher-js": "^7.0.2",
        "qrcode": "^1.4.4",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.29.0",
        "sass-loader": "10.*",
        "sinon": "^9.2.1",
        "stylelint": "^13.7.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-scss": "^3.18.0",
        "stylelint-webpack-plugin": "^2.1.0",
        "tempusdominus-core": "^5.19.0",
        "vanilla-lazyload": "^17.1.3",
        "vue": "^2.6.12",
        "vue-i18n": "^8.21.0",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "sweetalert2": "^10.9.0"
    }
}
